<%= form_for @slot do |s| %>
  <%= s.label :start %>: &nbsp; <%= s.text_field :start %> <br />
  <%= s.label :end %>: &nbsp; <%= s.text_field :end %> <br />
  <%= s.label :email %>&nbsp; <%= s.text_field :email %> <br />
  <%= s.hidden_field :name => :hour_id, :@hour.id %>
  <%= s.submit "add slot" %>
<% end %>

